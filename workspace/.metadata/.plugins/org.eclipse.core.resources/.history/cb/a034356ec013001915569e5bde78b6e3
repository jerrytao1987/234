package cn.itcast.demo;

public class TestStaticThread {
	
	public static void main(String[] args) {
		for (int i = 0; i < 20; i++) {
			new Thread(new Runnable() {
				
				@Override
				public void run() {
					TestClass testClass = StaticClass.getTestClass();
					System.out.println(testClass);
				}
			});
		}
	}
}
